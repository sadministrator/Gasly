{
  "name": "gasly",
  "version": "1.1.0",
  "description": "An Ethereum gas statistics API built with Node.js, Express, Typescript, and MongoDB.",
  "main": "app.ts",
  "scripts": {
    "start": "tsc && node --unhandled-rejections=strict ./build/app.js",
    "debugLinux": "export DEBUG=debug && npm run start",
    "debugWindows": "SET DEBUG=debug && npm run start",
    "test": "mocha -r ts-node/register 'test/**/*.test.ts' --unhandled-rejections=strict",
    "testDebug": "SET DEBUG=* && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sadministrator/gasly.git"
  },
  "keywords": [
    "API",
    "ethereum",
    "gas",
    "node.js",
    "express",
    "typescript",
    "mongodb"
  ],
  "author": "Marco Serrano",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/sadministrator/gasly/issues"
  },
  "homepage": "https://github.com/sadministrator/gasly#readme",
  "dependencies": {
    "axios": "^0.23.0",
    "cors": "^2.8.5",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.5.0",
    "express-winston": "^4.2.0",
    "helmet": "^4.6.0",
    "mongoose": "^6.0.12",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^5.1.3",
    "@types/mocha": "^9.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "mocha": "^9.1.3",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
